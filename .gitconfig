[user]
	name = Oswin Rodrigues
	email = <insert email here>
[color]
	ui = auto
	diff = auto
[alias]
	graph = log --all --graph --oneline --decorate=short
	hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph
	co = checkout
	cm = commit
	cam = commit -am
	camd = commit -a --amend
	st = status
	pl = pull
	plm = pull origin master
	ps = push
	psf = push -f
	pso = push -o ci.skip
	psfo = push -f -o ci.skip
	rb = rebase
	rbm = rebase master
[core]
	editor = code -w
